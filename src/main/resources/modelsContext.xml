<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/util
        http://www.springframework.org/schema/util/spring-util-3.2.xsd">

    <bean id="category" class="org.russow.model.Category"/>
    <bean id="contact" class="org.russow.model.Contact"/>
    <bean id="coupon" class="org.russow.model.Coupon"/>
    <bean id="customer" class="org.russow.model.Customer"/>
    <bean id="good" class="org.russow.model.Good"/>
    <bean id="order" class="org.russow.model.Order"/>
    <bean id="orderStatus" class="org.russow.model.OrderStatus"/>
    <bean id="role" class="org.russow.model.Role"/>
</beans>